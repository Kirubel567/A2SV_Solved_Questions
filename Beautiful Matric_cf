matrix = []
for i in range(5): 
    row = list(map(int, input().split()))
    matrix.append(row)
    
 
counter = 0
 
#find the 1 first
sub = 2
 
for i in range(len(matrix)): 
    for j in range(len(matrix[0])): 
        if matrix[i][j] == 1: 
            counter += abs(j-sub) + abs(i-sub)
 
print(counter)
