#User function Template for python3

class Solution:
    #Function to check if a is a subset of b.
    def isSubset(self, a, b):
        # Your code here
        freq = {}
        for ch in a: 
            freq[ch] = freq.get(ch, 0) + 1
        
        for ch in b: 
            if ch not in freq: 
                return False
            freq[ch] -= 1
            if freq[ch] < 0: 
                return False
        return True 
    
