class FrequencyTracker:

    def __init__(self):
        self.freq = {}
        self.freq_key = {} #the frequency as a key, for searching in hasFrequencey

    def add(self, number: int) -> None:
        old = self.freq.get(number, 0)
        self.freq[number] = self.freq.get(number, 0) + 1
        if old > 0: 
            self.freq_key[old] -= 1
        self.freq_key[self.freq[number]] = self.freq_key.get(self.freq[number],0) + 1
    
    def deleteOne(self, number: int) -> None:
        if number in self.freq: 
            self.freq_key[self.freq[number]] = self.freq_key.get(self.freq[number], 0) - 1

            self.freq[number] -= 1
            self.freq_key[self.freq[number]] = self.freq_key.get(self.freq[number], 0) + 1

            if self.freq[number] == 0:
                del self.freq[number]
        
        
    def hasFrequency(self, frequency: int) -> bool:
        return frequency in self.freq_key and self.freq_key[frequency] > 0


# Your FrequencyTracker object will be instantiated and called as such:
# obj = FrequencyTracker()
# obj.add(number)
# obj.deleteOne(number)
# param_3 = obj.hasFrequency(frequency)



